# Clawdbot Server Environment Configuration
# Copy this file to .env and update with your actual values

# Backend API URL (where the Transition OS backend is running)
# This is the backend that Clawdbot will communicate with
BACKEND_URL=http://localhost:8000

# Optional: API Key for backend authentication
# BACKEND_API_KEY=your-secret-api-key

# Clawdbot Server Configuration
CLAWDBOT_HOST=0.0.0.0
CLAWDBOT_PORT=8080

# Logging
LOG_LEVEL=INFO
